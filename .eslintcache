Selector(state => state.profile.uid);\r\n\r\n    useEffect(() => {\r\n        dataBase.ref('projects/').once('value')\r\n            .then(snapshot => dispatch(addProject(snapshot.val())))\r\n\r\n        dataBase.ref('requests/').once('value')\r\n            .then(snapshot => dispatch(addRequests(snapshot.val())))\r\n    }, [])\r\n\r\n    const myProjects = useMemo(() => {\r\n\r\n        return Object.values(requests).reduce((sum, el) => {\r\n            return el.status === \"accepted\" && el.userUid === profile ? [...sum, el.projectUid] : [...sum]\r\n        }, [])\r\n\r\n    }, [projects, requests, profile])\r\n\r\n    const project = Object.values(projects).filter(project => {\r\n        return myProjects.includes(project.projectId)\r\n    }).map(el => <ContentProjectsUserCard key={el.projectId} values={el} />)\r\n\r\n    return (\r\n        <div className=\"content__projects\">\r\n            {showProjects\r\n                ? project\r\n                : <PageAllProjectsForUser projects={projects} />\r\n            }\r\n        </div>\r\n    )\r\n}\r\n\r\nexport default ContentProjectsUser;","C:\\Users\\Trey\\Desktop\\stock-market\\my-app\\src\\components\\ContentProjectsUserCard\\ContentProjectsUserCard.js",[],"C:\\Users\\Trey\\Desktop\\stock-market\\my-app\\src\\components\\ContentProjectsCompany\\ContentProjectsCompany.js",["249"],"import { useEffect } from \"react\";\r\nimport { useDispatch, useSelector } from 'react-redux';\r\nimport {dataBase} from '../../firebase';\r\nimport {addRequests, AllProjects, ContentProjectsCompanyCard, addProject} from '../ContentProjectsCompany';\r\n\r\nconst ContentProjectsCompany = ({ showRequests }) => {\r\n\r\n    const profile = useSelector(state => state.profile);\r\n    const requests = useSelector(state => state.requests);\r\n    const dispatch = useDispatch();\r\n    \r\n    useEffect(() => {\r\n        dataBase.ref('projects/').orderByChild(\"profileUid\").equalTo(profile.uid).once('value', projects => {\r\n            dispatch(addProject(projects.val()))\r\n        });\r\n        dataBase.ref('requests/').orderByChild('companyUid').equalTo(profile.uid).once('value', snapshot => { getRequstsData(snapshot.val()) });\r\n    }, []) \r\n\r\n    const getRequstsData = async (snapshot) => {\r\n        const promises = Object.values(snapshot).map( el => {\r\n            return Promise.all([dataBase.ref('profiles/' + el.userUid).once('value'), dataBase.ref('projects/' + el.projectUid).once('value')])\r\n            .then(result => {\r\n                return {\r\n                    [el.requestUid]: {\r\n                        profile: result[0].val(),\r\n                        project: result[1].val(),\r\n                        ...el\r\n                    }\r\n                }\r\n            })\r\n        })\r\n\r\n        return Promise.all(promises)\r\n            .then(result => dispatch(addRequests(Object.values(result).reduce((prev, cur) => ({...prev, ...cur}), {}))));  \r\n    }\r\n\r\n    return (\r\n        <div className=\"content__projects\">\r\n            {showRequests\r\n                ? Object.keys(requests).map(requestUid => {\r\n                    return <ContentProjectsCompanyCard key={requestUid} values={requests[requestUid]}/>\r\n                })\r\n                :<AllProjects />\r\n            }  \r\n        </div>\r\n    )\r\n}\r\n\r\nexport default ContentProjectsCompany;","C:\\Users\\Trey\\Desktop\\stock-market\\my-app\\src\\components\\ContentProjectsCompanyCard\\ContentProjectsCompanyCard.js",[],"C:\\Users\\Trey\\Desktop\\stock-market\\my-app\\src\\components\\Loader\\Loader.js",[],"C:\\Users\\Trey\\Desktop\\stock-market\\my-app\\src\\actions\\addUser.js",[],"C:\\Users\\Trey\\Desktop\\stock-market\\my-app\\src\\reducers\\profile.js",[],"C:\\Users\\Trey\\Desktop\\stock-market\\my-app\\src\\providers\\UserProvider.js",[],"C:\\Users\\Trey\\Desktop\\stock-market\\my-app\\src\\components\\EditFormUser\\EditFormUser.js",[],"C:\\Users\\Trey\\Desktop\\stoc